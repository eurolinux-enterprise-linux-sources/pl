# MSG 		--- Executable (message) Objects

!include ..\makefile.def

MSGOBJS=	and.obj assign.obj binding.obj block.obj code.obj create.obj &
		equal.obj function.obj if.obj message.obj nonequal.obj &
		not.obj obtain.obj or.obj progn.obj quote.obj var.obj &
		when.obj while.obj nameref.obj

objects:	$(MSGOBJS)
		@%null

clean:
		rm -f *~ a.out core $(MSGOBJS) addlib
